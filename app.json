{
  "expo": {
    "name": "food-restaurant-search",
    "slug": "food-restaurant-search",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.food.zomatoali"
    },
    "description": "Food or Restaurant search app, which fetches data from Zomato API, and displays the result.",
    "githubUrl": "https://github.com/aliarslanansari/food-restaurant-search-react-native"
  }
}
